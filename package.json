{
  "name": "enterprise-reporting-system",
  "version": "1.0.0",
  "description": "Enterprise Reporting and Dashboard System with TanStack + Knex.js + shadcn/ui",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "db:migrate": "knex migrate:latest --knexfile src/lib/db/knexfile.ts",
    "db:migrate:make": "knex migrate:make --knexfile src/lib/db/knexfile.ts",
    "db:seed": "knex seed:run --knexfile src/lib/db/knexfile.ts",
    "db:rollback": "knex migrate:rollback --knexfile src/lib/db/knexfile.ts",
    "jobs:worker": "tsx src/lib/jobs/worker-runner.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "test:performance": "vitest run --dir tests/performance --pool=forks --poolOptions.forks.singleFork",
    "test:all": "vitest run --coverage",
    "test:seed": "tsx tests/setup/seed-test-database.ts",
    "pretest": "npm run test:seed"
  },
  "dependencies": {
    "@auth/core": "^0.34.2",
    "@hookform/resolvers": "^3.9.0",
    "@monaco-editor/react": "^4.6.0",
    "@radix-ui/react-accordion": "^1.2.0",
