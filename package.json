{
  "name": "enterprise-reporting-system",
  "version": "1.0.0",
  "description": "Enterprise Reporting and Dashboard System with TanStack + Knex.js + shadcn/ui",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4050",
    "build": "npm run lint && npm run typecheck && next build",
    "start": "next start -p 4050",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "db:migrate": "knex migrate:latest --knexfile src/lib/db/knexfile.ts",
    "db:migrate:make": "knex migrate:make --knexfile src/lib/db/knexfile.ts",
    "db:seed": "knex seed:run --knexfile src/lib/db/knexfile.ts",
    "db:rollback": "knex migrate:rollback --knexfile src/lib/db/knexfile.ts",
    "db:sample": "tsx src/lib/db/sample-data/seed.ts",
    "jobs:worker": "tsx src/lib/jobs/worker-runner.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test:setup": "tsx tests/setup-data.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:all": "npm run test:setup && npm run test:e2e",
    "test:ci": "npm run lint && npm run typecheck && npm run test:setup && npm run test:e2e",
    "test:watch": "playwright test --watch",
    "precommit": "npm run lint && npm run typecheck && npm run format:check",
    "prepare": "npm run build",
    "test:phase1": "playwright test --grep '@batch1' --workers=1",
    "test:phase2": "playwright test --grep '@batch2' --workers=1",
    "test:phase3": "playwright test --grep '@batch3' --workers=1",
    "test:phase4": "playwright test --grep '@batch4' --workers=1",
    "test:phase5": "playwright test --grep '@batch5' --workers=1",
    "test:phase6": "playwright test --grep '@batch6' --workers=1",
    "test:batches": "for i in 1 2 3 4 5 6; do echo \"Running Phase $i...\" && npm run test:phase$i || exit 1; done",
    "test:app": "playwright test e2e/app.spec.ts --workers=1",
    "test:dashboard": "playwright test e2e/dashboards.spec.ts --workers=1",
    "test:sql": "playwright test e2e/sql-editor.spec.ts --workers=1",
    "test:reports": "playwright test e2e/reports.spec.ts --workers=1",
    "test:charts": "playwright test e2e/charts.spec.ts --workers=1"
  },
  "dependencies": {
    "@auth/core": "^0.34.2",
    "@bull-board/api": "^6.16.2",
    "@bull-board/ui": "^6.16.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^3.9.0",
    "@monaco-editor/react": "^4.6.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-table": "^8.20.5",
    "@tanstack/react-virtual": "^3.10.7",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.3.0",
    "bullmq": "^5.12.15",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "exceljs": "^4.4.0",
    "ioredis": "^5.4.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.441.0",
    "monaco-editor": "^0.51.0",
    "nanoid": "^5.0.7",
    "next": "14.2.11",
    "next-auth": "^5.0.0-beta.21",
    "next-themes": "^0.3.0",
    "node-sql-parser": "^5.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^1.4.4",
    "react-hook-form": "^7.53.0",
    "react-resizable-panels": "^4.4.1",
    "recharts": "^2.12.7",
    "sonner": "^1.5.0",
    "sql-formatter": "^15.4.2",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.11",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-grid-layout": "^1.3.5",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.11",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.11",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
